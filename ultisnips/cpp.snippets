snippet guard "header guard"
#ifndef $1_H 
#define $1_H 
$0
#endif
endsnippet

snippet struct "struct"
typedef struct $1
{
	$2
}$3;
$0
endsnippet

snippet pfun "function"
$1 $2 ($3);$0
endsnippet

snippet inc "headers"
#include <stdio.h>
#include <stdlib.h>
endsnippet

snippet incf "header file"
#include "$1"$0
endsnippet
